doctype html
html
  head
    include shared/head
  body
    .main
      include shared/header

      .filter
        .pull-left
          nav.nav.nav__type-filter-left
            ul.nav-list
              li.nav-item
                a.nav-link.nav-link__active(href="#") Cache games
              li.nav-item
                a.nav-link(href="#") Tourney
              li.nav-item
                a.nav-link(href="#") Sit'n'Go
        .pull-right
          nav.nav.nav__type-filter-right
            ul.nav-list
              li.nav-item
                a.nav-link(href="#")
                  | My tables
                  span.nav-digit 2
              li.nav-item
                a.nav-link.nav-link__active.js-filter-button(href="#") Filter
        .clearfix

      .filter-content.js-filter-content
        .filter-labels
          .filter-label maximum players
          .filter-label game type
          .filter-label stakes
          .filter-label buy in
          .filter-label sort by
          .filter-label advanced

        .filter-controls
          .filter-control-group
            nav.nav.nav__type-players
              ul.nav-list
                li.nav-item
                  a.nav-link.nav-link__active(href="#") 2
                li.nav-item
                  a.nav-link(href="#") 3
                li.nav-item
                  a.nav-link(href="#") 5
                li.nav-item
                  a.nav-link(href="#") 9

          .filter-control-group
            .filter-select
              | Hold'em
              .header-user-caret.header-user-caret__at-select

          .filter-control-group
            .filter-range
              .filter-range-stripe(style="width: 50%; left: 25%;")
              .filter-range-drag(style="left: 25%;")
                .filter-range-drag-label $ 0.4
              .filter-range-drag(style="left: 75%;")
                .filter-range-drag-label $ 0.6

          .filter-control-group
            .filter-range
              .filter-range-stripe(style="width: 100%; left: 0%;")
              .filter-range-drag(style="left: 0;")
              .filter-range-drag(style="left: 100%;")
              .filter-range-drag-label-default No limit

          .filter-control-group
            nav.nav.nav__type-stakes
              ul.nav-list
                li.nav-item
                  a.nav-link.nav-link__active(href="#") Stakes
                li.nav-item
                  a.nav-link(href="#") Avg.
                li.nav-item
                  a.nav-link(href="#") Pot

          .filter-control-group
            nav.nav.nav__type-advanced
              ul.nav-list
                li.nav-item
                  a.nav-link(href="#") Clear all


      .lobby-list.lobby-list__display-table
        .lobby-list-header
          .lobby-list-header-item
            | limit
            span.lobby-list-header-item-help ?
          .lobby-list-header-item
            | tables
            span.lobby-list-header-item-help ?
          .lobby-list-header-item
            | game type
            span.lobby-list-header-item-help ?
          .lobby-list-header-item
            | seats
            span.lobby-list-header-item-help ?
          .lobby-list-header-item
            | stakes
            span.lobby-list-header-item-help ?
          .lobby-list-header-item
            | buy in
            span.lobby-list-header-item-help ?
          .lobby-list-header-item
            span.lobby-list-header-item-help
            | hide full
            .lobby-view-switcher
              .lobby-view.lobby-view__type-table.lobby-view__state-active
              .lobby-view.lobby-view__type-list
              .lobby-view.lobby-view__type-settings


        - var n = 0
        while n < 4
          .lobby-item
            .lobby-item-content 2/4 - 5/10
            .lobby-item-content Table 1
            .lobby-item-content Hold'em
            .lobby-item-content.lobby-item-content__filled 9/9
            .lobby-item-content $ 0.4 / $ 0.6
            .lobby-item-content $ 6.40 / $ 16
            .lobby-item-content
              a.lobby-list-button(href="javascript:void(0);") Watch
              a.lobby-list-button.lobby-list-button__type-disabled(href="javascript:void(0);") Full table

          .lobby-item
            .lobby-item-content 2/4 - 5/10
            .lobby-item-content Table 2
            .lobby-item-content Hold'em
            .lobby-item-content 2/9
            .lobby-item-content $ 0.4 / $ 0.6
            .lobby-item-content $ 6.40 / $ 16
            .lobby-item-content
              a.lobby-list-button(href="javascript:void(0);") Watch
              a.lobby-list-button.lobby-list-button__type-sucess(href="javascript:void(0);") Sit Here

          .lobby-item
            .lobby-item-content 2/4 - 5/10
            .lobby-item-content James Bond' Table
            .lobby-item-content Omaha No limit
            .lobby-item-content 4/9
            .lobby-item-content $ 0.4 / $ 0.6
            .lobby-item-content $ 6.40 / $ 16
            .lobby-item-content
              a.lobby-list-button(href="javascript:void(0);") Watch
              a.lobby-list-button.lobby-list-button__type-sucess(href="javascript:void(0);") Sit Here

          .lobby-item
            .lobby-item-content 2/4 - 5/10
            .lobby-item-content Joe Table 1
            .lobby-item-content Hold'em
            .lobby-item-content 4/9
            .lobby-item-content $ 0.4 / $ 0.6
            .lobby-item-content $ 6.40 / $ 16
            .lobby-item-content
              a.lobby-list-button(href="javascript:void(0);") Watch
              a.lobby-list-button.lobby-list-button__type-sucess(href="javascript:void(0);") Sit Here

          .lobby-item
            .lobby-item-content 2/4 - 5/10
            .lobby-item-content London Table
            .lobby-item-content Hold'em
            .lobby-item-content.lobby-item-content__almost-fill 8/9
            .lobby-item-content $ 0.4 / $ 0.6
            .lobby-item-content $ 6.40 / $ 16
            .lobby-item-content
              a.lobby-list-button(href="javascript:void(0);") Watch
              a.lobby-list-button.lobby-list-button__type-sucess(href="javascript:void(0);") Sit Here

          .lobby-item.lobby-item__type-active
            .lobby-item-content 2/4 - 5/10
            .lobby-item-content
              | table 8
              span.lobby-item-yours (yours)
            .lobby-item-content Hold'em
            .lobby-item-content 3/9
            .lobby-item-content $ 0.4 / $ 0.6
            .lobby-item-content $ 6.40 / $ 16
            .lobby-item-content
              a.lobby-list-button(href="javascript:void(0);") Watch
              a.lobby-list-button.lobby-list-button__type-primary(href="javascript:void(0);") Sit Out

          - n++

    script(src="https://code.jquery.com/jquery-2.1.3.min.js")
    script(src="/js/application.js")
